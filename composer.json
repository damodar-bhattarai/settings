{
    "name": "stillalive/settings",
    "description": "Set settings on the fly",
    "keywords": [
        "stillalive",
        "settings"
    ],
    "homepage": "https://github.com/stillalive/settings",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Damodar Bhattarai",
            "email": "info@bdamodar.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Stillalive\\Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stillalive\\Settings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stillalive\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Stillalive\\Settings\\SettingsFacade"
            }
        }
    }
}
