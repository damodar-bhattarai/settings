    "type": "library",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stillalive\\Settings\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stillalive\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Damodar Bhattarai",
            "email": "damodar.bhattarai.1999@gmail.com"
        }
    ],
    "minimum-stability": "dev"
