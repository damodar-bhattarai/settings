{
    "name": "stillalive/settings",
    "description": "Package to store & retrive commonly used variables like company name, email, phone , etc",
    "keywords": [
        "StillAlive",
        "laravel",
        "settings"
    ],
    "homepage": "https://github.com/stillalive/settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Damodar Bhattarai",
            "email": "info@bdamodar.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "StillAlive\\Settings\\": "src",
            "StillAlive\\Settings\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StillAlive\\Settings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StillAlive\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "StillAlive\\Settings\\Facades\\Settings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
